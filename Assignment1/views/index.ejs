    var Index = 0;
    cycle();

    function cycle(){
        var i;
        var x = document.getElementByClassName("Slideshow");
        for(i = 0; i < x.length; i++) {
            x[i].style.display = "none";
        }
        Index++;
        if (Index > x.length) {Index = 1}
        x[Index - 1].style.display = "block";
        setTimeout(cycle, 5000);
    }

